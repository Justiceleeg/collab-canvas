{
	"rules": {
		".read": false,
		".write": false,
		"presence": {
			".read": "auth != null",
			"$userId": {
				".write": "auth != null && auth.uid === $userId",
				".validate": "newData.hasChildren(['displayName', 'color', 'online'])"
			}
		},
		"test": {
			".read": true,
			".write": true
		}
	}
}