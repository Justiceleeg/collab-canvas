{
	"rules": {
		".read": false,
		".write": false,
		"presence": {
			".read": "auth != null",
			"$userId": {
				".read": "auth != null",
				".write": "auth != null && auth.uid === $userId",
				"$field": {
					".write": "auth != null && auth.uid === $userId"
				}
			}
		},
		"test": {
			".read": true,
			".write": true
		}
	}
}